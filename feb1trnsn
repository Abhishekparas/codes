#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
    int n,i;
    cin>>n;
    long long int a[n],sum=0;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    sort(a,a+n);
    for(i=0;i<n-1;i++)
    {
        if(a[i]!=a[i+1])
        {
            sum++;
        }
        //cout<<a[i]<<" ";
    }
    cout<<sum<<endl;
	// your code goes here
	return 0;
}
