#include<bits/stdc++.h>
using namespace std;
# define ll long long 
int main(){
    
    ll n;
    cin>>n;
    ll arr[n];
    for(ll i=1; i<=n; i++){
        cin>>arr[i];
    }
    ll q;
    cin>>q;
    while(q--){
        ll a,b;
        ll x=0;
        cin>>a>>b;
        for(ll i=a; i<=b; i++){
            x = x^arr[i];
        }
        cout<<x<<endl;

    }
    return 0;
}
