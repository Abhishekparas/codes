#include <iostream>
#include <math.h>
using namespace std;
int main() {
    int N;
    int  num;
    cin>>N;
    for(int i=0;i<N;i++){
        cin>>num;
		int rem;
	int sum = 0;
	int j = 0;
	while(num>0){
		rem = num%10;
		sum = sum + rem *pow(2,j);
		num = num/10;
		j++;
		


	}
	cout<<sum;
        cout<<endl;
    }

}
